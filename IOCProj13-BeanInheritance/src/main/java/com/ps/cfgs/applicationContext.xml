<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="baseCar" class="com.ps.beans.Car" abstract="true">
		<property name="modelName" value="Swift"/>
		<property name="color" value="red"/>
		<property name="engineCc" value="1200CC"/>
		<property name="makeCompany" value="MarutiSuzuki"/>
		<property name="type" value="sedan"/>
		<property name="fuelType" value="Diesel"/>
	</bean>
	
	<bean id="rajaCar" class="com.ps.beans.Car" parent="baseCar">
		<property name="regNo" value="TS09JK4577"/>
		<property name="ownerName" value="raja"/>		
	</bean>
		
	<bean id="raniCar" class="com.ps.beans.Car" parent="baseCar">
		<property name="regNo" value="TS09HK7676"/>
		<property name="ownerName" value="rani"/>
	</bean>

</beans>
